{
  "hosting": [
    {
      "target": "domino-prod",
      "site": "eldominoazul",
      "public": "dist/domino",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "domino-backup",
      "site": "eldominoazul2",
      "public": "dist/domino",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ]
}
